        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="android:layout_marginTop">@dimen/_20sdp</item>
        <item name="android:layout_marginStart">@dimen/_24sdp</item>
    </style>


    <!--_____________________________ImageViewStyle_______________________________________-->

    <style name="DefImageViewStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">?android:selectableItemBackgroundBorderless</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
    </style>

