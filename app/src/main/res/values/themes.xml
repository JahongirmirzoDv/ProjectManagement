    <!--________________LayoutStyles_____________________-->
    <style name="DefaultLayoutStyle">
        <item name="android:layout_marginTop">@dimen/_8sdp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/white</item>
    </style>


    <style name="DefaultStyle" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@color/colorDarkBlue</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorSecondary">#0BACC1</item>
