        <item name="android:letterSpacing">0</item>
        <item name="android:paddingStart">0dp</item>
        <item name="android:textSize">@dimen/_11ssp</item>
    </style>

    <style name="CreateScreensTextTime">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/_5sdp</item>
        <item name="android:textSize">@dimen/_11ssp</item>
    </style>

    <style name="CreateScreensFieldTitle" parent="TextSmallSemiBold">
        <item name="android:layout_marginStart">@dimen/_24sdp</item>
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="DashboardStatisticTextStyle" parent="DashboardTextStyle">
        <item name="android:layout_marginHorizontal" tools:ignore="NewApi">@dimen/_35sdp</item>
    </style>

    <style name="CreateScreensMemberTextStyle" parent="TextSmall">
