    </style>

    <style name="ImageViewOthersStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="ImageLogoStyle" parent="ImageViewDefStyle">
        <item name="contentDescription">@string/app_name</item>
        <item name="android:layout_width">@dimen/_90sdp</item>
        <item name="android:layout_marginTop">@dimen/_10sdp</item>
        <item name="android:layout_height">@dimen/_33sdp</item>
    </style>


    <style name="OthersImageIconStyle" parent="ImageViewDefStyle">
        <item name="android:layout_width">@dimen/_30sdp</item>
        <item name="contentDescription">@string/app_name</item>
        <item name="android:layout_height">@dimen/_30sdp</item>
        <item name="android:layout_marginStart">@dimen/_15sdp</item>
        <item name="android:layout_marginVertical" tools:ignore="NewApi">@dimen/_10sdp</item>
    </style>


    <!--                       CreateScreensStyle                           -->
    <style name="CreateTickImageStyle" parent="ImageViewDefStyle">
        <item name="android:layout_width">@dimen/_35sdp</item>
        <item name="android:layout_height">@dimen/_35sdp</item>
