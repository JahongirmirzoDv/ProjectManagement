        <item name="android:layout_marginTop">@dimen/_40sdp</item>
        <item name="android:layout_marginBottom">@dimen/_20sdp</item>
    </style>


    <style name="GeneralBlueButtonStyle" parent="ButtonDefStyle">
        <item name="cornerRadius">@dimen/_4sdp</item>
        <item name="android:layout_marginHorizontal" tools:ignore="NewApi">@dimen/_17sdp</item>
        <item name="android:layout_marginStart">@dimen/_17sdp</item>
        <item name="android:layout_marginEnd">@dimen/_17sdp</item>
