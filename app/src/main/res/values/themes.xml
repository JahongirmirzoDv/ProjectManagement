    </style>

    <style name="CreateScreensAddButton" parent="DefImageViewStyle">
        <item name="android:layout_marginEnd">@dimen/_24sdp</item>
        <item name="android:contentDescription">@string/app_name</item>
    </style>

    <style name="CreateScreensDateImageIconStyle" parent="DefImageViewStyle">
        <item name="android:layout_marginStart">@dimen/_5sdp</item>
        <item name="android:layout_marginTop">@dimen/_24sdp</item>
    </style>


    <!--_____________________________ButtonStyle_______________________________________-->
    <style name="ButtonDefStyle">
        <item name="android:layout_width">@dimen/_133sdp</item>
