<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ProjectManagement" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">#364F6B</item>
        <item name="colorSecondaryVariant">#364F6B</item>
        <item name="colorOnSecondary">@color/black</item>

        <!-- Status bar color. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:statusBarColor" tools:targetApi="l">@color/white</item>
        <item name="android:windowLightStatusBar" tools:ignore="NewApi">true</item>

        <!-- Customize your theme here. -->
        <item name="alertDialogTheme">@style/AlertDialogTheme</item>
        <item name="android:datePickerDialogTheme">@style/MyDatePickerDialogTheme</item>
    </style>


    <!--___________________TextStyle_______________________________________-->

    <style name="TextMedium">
        <item name="contentDescription">@string/app_name</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textSize">@dimen/_13ssp</item>
        <item name="android:letterSpacing">0.02</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:paddingRight">@dimen/_4sdp</item>
        <item name="android:paddingLeft">@dimen/_4sdp</item>
        <item name="android:layout_marginTop">@dimen/_8sdp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_marginBottom">@dimen/_8sdp</item>
    </style>


    <style name="TextMediumSemiBold" parent="TextMedium">
        <item name="android:fontFamily">@font/inter_semibold</item>
    </style>

    <style name="TextSmall" parent="TextMedium">
        <item name="contentDescription">@string/app_name</item>
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textSize">@dimen/_11ssp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:ellipsize">end</item>
        <item name="maxLine">1</item>
    </style>

    <style name="TextSmallWithPercentage" parent="TextSmall">
        <item name="android:textColor">@color/hintColor</item>
        <item name="android:drawablePadding">@dimen/_5sdp</item>
        <item name="android:drawableStart">@drawable/ic_percent_goal</item>
    </style>

    <style name="TextSmallSemiBold" parent="TextMedium">
        <item name="contentDescription">@string/app_name</item>
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:textSize">@dimen/_11ssp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">normal</item>
    </style>




    <style name="TextLarge" parent="TextMedium">
        <item name="contentDescription">@string/app_name</item>
        <item name="android:fontFamily">@font/inter_medium</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">@dimen/_16ssp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TextSmallAddButtonTextStyle" parent="TextSmall">
        <item name="android:background">@drawable/back_add_worker</item>
        <item name="android:drawablePadding">@dimen/_5sdp</item>
        <item name="android:elevation">@dimen/_5sdp</item>
        <item name="android:padding">@dimen/_10sdp</item>
        <item name="drawableRightCompat">@drawable/ic_add_plus</item>
        <item name="drawableTint">@color/hintColor</item>
        <item name="android:layout_marginBottom">@dimen/_12sdp</item>
        <item name="android:textColor">@color/text_color</item>
        <!--        <item name="android:drawableTint"></item>-->
    </style>

    <style name="TextLargeInterBold" parent="TextLarge">
        <item name="contentDescription">@string/app_name</item>
        <item name="android:fontFamily">@font/inter_bold</item>
    </style>

    <style name="OtherScreenTextStyle" parent="TextSmall">
        <item name="android:layout_marginStart">@dimen/_16sdp</item>
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="DashboardTextStyle" parent="TextMedium">
        <item name="fontFamily">@font/inter_medium</item>
        <item name="android:layout_width">@dimen/size_0dp</item>
        <item name="android:layout_margin">@dimen/_21sdp</item>
        <item name="android:elevation">@dimen/_7sdp</item>
    </style>

    <style name="GreenTextStyle" parent="TextMedium">
        <item name="android:textSize">@dimen/_18ssp</item>
        <item name="android:textColor">@color/green</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
    </style>


    <!--                                    CreateScreensStyle                      -->

    <style name="CreateToolbarTitleStyle" parent="TextLarge">
